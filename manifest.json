{
"update_url": "https://clients2.google.com/service/update2/crx",

    "name": "Ooh!",
    "version": "1.0.0",
    "description": "Plays the Minecraft ooh sound on tab close! Credit to the Roblox OOF extention!",
    "permissions": ["tabs", "storage"],
    "background": {
      "scripts": ["main.js"],
      "persistent": false
    },
	"icons": {"48": "icon48.png"},
	"browser_action": { 
        "default_icon": {"48": "icon48.png",
						"128": "icon128.png"},
		"default_popup": "popup.html" 		
    },
    "manifest_version": 2
}